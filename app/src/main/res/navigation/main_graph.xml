<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/navigation_success_login">


    <fragment
        android:id="@+id/navigation_blogs"
        android:name="edu.uw.tcss450.mylabappadamhh.ui.blog.BlogListFragment"
        android:label="@string/title_blogs"
        tools:layout="@layout/fragment_blog_list">
        <action
            android:id="@+id/action_blogListFragment_to_blogPostFragment"
            app:destination="@id/blogPostFragment" />
    </fragment>
    <fragment
        android:id="@+id/blogPostFragment"
        android:name="edu.uw.tcss450.mylabappadamhh.ui.blog.BlogPostFragment"
        android:label="@string/title_blogs" >
        <argument
            android:name="blog"
            app:argType="edu.uw.tcss450.mylabappadamhh.ui.blog.BlogPost" />
    </fragment>
    <fragment
        android:id="@+id/navigation_success_login"
        android:name="edu.uw.tcss450.mylabappadamhh.ui.home.SuccessLoginFragment"
        android:label="@string/title_success"
        tools:layout="@layout/fragment_success_login" />
</navigation>